{
  "version": "0.0.0",
  "private": true,
  "name": "angular-upgrading",
  "description": "Upgrading from Angular v1 to v2",
  "license": "MIT",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "concurrently \"npm run tsc:w\" \"npm run server\" ",
    "server": "http-server -a 0.0.0.0 -p 8000",
    "typings": "typings",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angular/upgrade": "2.1.2",
    "@types/angular": "^1.5.19",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.40",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "typescript": "^2.0.6",
    "typings": "^1.5.0"
  }
}
